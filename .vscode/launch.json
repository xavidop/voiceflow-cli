{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Version command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "version"
            ]
        },
        {
            "name": "Test suite command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "test", "execute", "examples/"
            ]
        },
        {
            "name": "Fetch all transcripts command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "transcript", "fetch-all", "--agent-id", "67213ee4d6009327e23018a9", "--tag", "6777dedec07cff77a659a017"
            ]
        },
        {
            "name": "Export agent command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "agent", "export", "--version-id", "development", "--agent-id", "67213ee4d6009327e23018a9"
            ]
        },
        {
            "name": "Fetch analytics command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "analytics", "fetch", "--agent-id", "67213ee4d6009327e23018a9"
            ]
        },
        {
            "name": "KB Query command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "kb", "query", "--model", "claude-3-haiku", "--question", "What is voiceflow?"
            ]
        },
        {
            "name": "Upload Document URL command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "document", "upload-url", "--url", "https://www.voiceflow.com/", "--name", "Vocieflow Page"
            ]
        },
        ,
        {
            "name": "Upload Document File command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "ENV": "development"
            },
            "args": [
                "document", "upload-file", "--file", "./db.pdf"
            ]
        }
    ]
}