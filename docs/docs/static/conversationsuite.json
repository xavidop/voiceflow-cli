{
		"$schema": "https://json-schema.org/draft/2020-12/schema",
		"$id": "https://github.com/xavidop/voiceflow-cli/internal/types/tests/suite",
		"$ref": "#/$defs/Suite",
		"$defs": {
			"Suite": {
				"properties": {
					"name": {
						"type": "string"
					},
					"description": {
						"type": "string"
					},
					"environmentName": {
						"type": "string"
					},
					"tests": {
						"items": {
							"$ref": "#/$defs/TestFile"
						},
						"type": "array"
					}
				},
				"additionalProperties": false,
				"type": "object",
				"required": [
					"name",
					"description",
					"environmentName",
					"tests"
				]
			},
			"TestFile": {
				"properties": {
					"id": {
						"type": "string"
					},
					"file": {
						"type": "string"
					}
				},
				"additionalProperties": false,
				"type": "object",
				"required": [
					"id",
					"file"
				]
			},
			"Tests": {
				"$schema": "https://json-schema.org/draft/2020-12/schema",
				"$defs": {
					"Agent": {
						"properties": {
							"validate": {
								"items": {
									"$ref": "#/$defs/Validation"
								},
								"type": "array"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"validate"
						]
					},
					"AgentTest": {
						"properties": {
							"goal": {
								"type": "string"
							},
							"persona": {
								"type": "string"
							},
							"maxSteps": {
								"type": "integer"
							},
							"userInformation": {
								"items": {
									"$ref": "#/$defs/UserInfo"
								},
								"type": "array"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"goal",
							"persona",
							"maxSteps"
						]
					},
					"Interaction": {
						"properties": {
							"id": {
								"type": "string"
							},
							"user": {
								"$ref": "#/$defs/User"
							},
							"agent": {
								"$ref": "#/$defs/Agent"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"id",
							"user",
							"agent"
						]
					},
					"SimilarityConfig": {
						"properties": {
							"provider": {
								"type": "string"
							},
							"model": {
								"type": "string"
							},
							"temperature": {
								"type": "number"
							},
							"similarityThreshold": {
								"type": "number"
							},
							"top_k": {
								"type": "integer"
							},
							"top_p": {
								"type": "number"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"provider",
							"model",
							"temperature",
							"similarityThreshold",
							"top_k",
							"top_p"
						]
					},
					"Test": {
						"properties": {
							"name": {
								"type": "string"
							},
							"description": {
								"type": "string"
							},
							"interactions": {
								"items": {
									"$ref": "#/$defs/Interaction"
								},
								"type": "array"
							},
							"agent": {
								"$ref": "#/$defs/AgentTest"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"name",
							"description"
						]
					},
					"User": {
						"properties": {
							"type": {
								"type": "string"
							},
							"text": {
								"type": "string"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"type"
						]
					},
					"UserInfo": {
						"properties": {
							"name": {
								"type": "string"
							},
							"value": {
								"type": "string"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"name",
							"value"
						]
					},
					"Validation": {
						"properties": {
							"id": {
								"type": "string"
							},
							"type": {
								"type": "string"
							},
							"value": {
								"type": "string"
							},
							"values": {
								"items": {
									"type": "string"
								},
								"type": "array"
							},
							"similarityConfig": {
								"$ref": "#/$defs/SimilarityConfig"
							},
							"variableConfig": {
								"$ref": "#/$defs/VariableConfig"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"type"
						]
					},
					"VariableConfig": {
						"properties": {
							"name": {
								"type": "string"
							},
							"jsonPath": {
								"type": "string"
							}
						},
						"additionalProperties": false,
						"type": "object",
						"required": [
							"name"
						]
					}
				},
				"items": {
					"$ref": "#/$defs/Test"
				},
				"type": "array"
			}
		},
		"description": "voiceflow-cli suite definition file"
	}